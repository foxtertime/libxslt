+	@./testTargets $(srcdir)
+
+fuzz-xpath: xpath$(EXEEXT)
+	@mkdir -p corpus/xpath
+	./xpath$(EXEEXT) \
+            -max_len=256 \
+            -dict=$(srcdir)/xpath.dict \
+	    corpus/xpath $(srcdir)/seed/xpath
+
+fuzz-xslt: xslt$(EXEEXT)
+	@mkdir -p corpus/xslt
+	./xslt$(EXEEXT) \
+            -dict=$(srcdir)/xslt.dict \
+	    corpus/xslt $(srcdir)/seed/xslt
+
